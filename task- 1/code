#include<iostream>

int main()
{
    
    int steps,x;
    std::cout<<"ENTER THE VALUE OF X: ";
    std::cin>>x;
    if(x<=5)
    {
        steps=1;
    }
    else if(x%5==0)
    {
        steps=x/5;
    }
    else
        steps=(x/5)+1;
        
    std::cout<<"THE MINIMUM NUMBER OF STEPS REQUIRED ARE "<<steps;
    return 0;
}
